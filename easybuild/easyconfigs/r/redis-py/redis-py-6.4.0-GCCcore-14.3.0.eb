easyblock = 'PythonBundle'

name = 'redis-py'
version = '6.4.0'

homepage = 'https://github.com/redis/redis-py'
description = "The Python interface to the Redis key-value store."

toolchain = {'name': 'GCCcore', 'version': '14.3.0'}

builddependencies = [
    ('binutils', '2.44'),
    ('hatchling', '1.27.0'),
]

dependencies = [
    ('Python', '3.13.5'),
    ('typing-extensions', '4.14.1'),
    ('Redis', '8.2.2'),
]

exts_list = [
    ('async_timeout', '5.0.1', {
        'checksums': ['d9321a7a3d5a6a5e187e824d2fa0793ce379a202935782d555d6e9d2735677d3'],
    }),
    (name, version, {
        'modulename': 'redis',
        'source_urls': ['https://github.com/redis/redis-py/archive/refs/tags/'],
        'sources': ['v%(version)s.tar.gz'],
        'checksums': ['7ce2a05bc9888af3bb7c4c11f3dcccefe8b66136d3248c0ef86eb6d412660419'],
    }),
]

moduleclass = "data"
